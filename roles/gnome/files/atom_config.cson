"*":
  welcome:
    showOnStartup: false
  autosave:
    enabled: true
  blame: {}
  core:
    autoHideMenuBar: true
    excludeVcsIgnoredPaths: false
    ignoredNames: [
      "*.pyc"
    ]
    packagesWithKeymapsDisabled: [
      "vim-mode"
    ]
    projectHome: "/home/michael/Development"
    themes: [
      "nucleus-dark-ui"
      "seti-syntax"
    ]
  editor:
    fontSize: 13
    invisibles: {}
    preferredLineLength: 120
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
    tabType: "soft"
  "keybinding-cheatsheet": {}
  linter: {}
  "linter-flake8":
    ignoreErrorCodes: [
      "E702"
    ]
    maxLineLength: 120
  "linter-shellcheck":
    shellcheckExecutablePath: "/usr/bin/shellcheck"
  minimap: {}
  "python-tools": {}
  "rst-preview": {}
  "spell-check": {}
  "tree-view":
    hideVcsIgnoredFiles: true
  "vim-mode":
    useClipboardAsDefaultRegister: true
  welcome:
    showOnStartup: false
