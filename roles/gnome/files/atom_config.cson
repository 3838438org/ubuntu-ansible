"*":
  autosave:
    enabled: true
  blame: {}
  core:
    autoHideMenuBar: true
    customFileTypes:
      "text.html.jinja": [
        "jinja"
      ]
      "text.xml": [
        "glade"
      ]
    excludeVcsIgnoredPaths: true
    ignoredNames: [
      "*.pyc"
      "__pycache__"
    ]
    packagesWithKeymapsDisabled: [
      "vim-mode"
    ]
    themes: [
      "nucleus-dark-ui"
      "atom-dark-fusion-syntax"
    ]
  editor:
    fontSize: 14
    invisibles: {}
    preferredLineLength: 100
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
    tabType: "soft"
  "ini/open_pytest_file": false
  "init/open_pytest_file": true
  "keybinding-cheatsheet": {}
  linter: {}
  "linter-flake8":
    ignoreErrorCodes: [
      "E702"
    ]
    maxLineLength: 120
  "linter-shellcheck":
    shellcheckExecutablePath: "/usr/bin/shellcheck"
  minimap: {}
  "pdf-view": {}
  "python-tools": {}
  "rst-preview": {}
  "spell-check": {}
  "tree-view":
    hideIgnoredNames: true
    hideVcsIgnoredFiles: true
  "vim-mode":
    useClipboardAsDefaultRegister: true
  welcome:
    showOnStartup: false
